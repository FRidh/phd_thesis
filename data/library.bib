@inproceedings{Maffei2014,
abstract = {The positive influence of parks and public open spaces on health and human wellness has been studied by several researches and they highlighted how the improvement of the soundscape contributes to a better overall environmental quality. However, the human perception has a
multisensory nature and, therefore, holistic analyses are necessary to estimate the influence by sound, vision, odours, meteorological conditions and psychological components on the satisfaction of the users of these open spaces. The waterfronts, with their wide variety of features, are almost unexplored areas from the point of view of the sound and environmental perception,
although they are usually treated with special care by the stakeholders in the urban renewal planning for their high commercial, tourist and recreational potentialities. This paper introduces the first step on the development of a methodology for the perceptual characterization of the
waterfront soundscapes. The methodology has been applied to the sea front of the city of Naples,
which is under a strong urban transformation, and preliminary results are outlined.},
address = {Krakow},
author = {Maffei, Luigi and {Puyana Romero}, Virginia and Brambilla, Giovanni and di Gabriele, Maria and Gallo, Veronica},
booktitle = {Forum Acusticum 2014},
title = {{Characterization of the soundscape of urban waterfronts}},
year = {2014}
}
@article{Hao2014,
abstract = {The influence of urban morphology of low-density built-up areas on spatial noise level attenuation of flyover aircrafts is investigated at a mesoscale. Six urban morphological parameters, including Building Plan Area Fraction, Complete Aspect Ratio, Building Surface Area to Plan Area Ratio, Building Frontal Area Index, Height-to-Width Ratio, and Horizontal Distance of First-row Building to Flight Path, have been selected and developed. Effects of flight altitude and horizontal flight path distance to site, on spatial aircraft noise attenuation, are examined, considering open areas and fa{\c{c}}ades. Twenty sampled sites, each of 250m*250m, are considered. The results show that within 1000m horizontal distance of flight path to a site, urban morphology plays an important role in open areas, especially for the buildings with high sound absorption fa{\c{c}}ades, where the variance of average noise level attenuation among different sites is about 4.6dB at 3150Hz. The effect of flight altitude of 200–400ft on average noise level attenuation is not significant, within about 2dB at both 630Hz and1600Hz in open areas. Urban morphological parameters influence the noise attenuation more in open areas than that on fa{\c{c}}ades. Spatial noise attenuation of flyover aircrafts is mainly correlated to Building Frontal Area Index and Horizontal Distance of First-row Building to Flight Path.},
author = {Hao, Yiying and Kang, Jian},
issn = {0003682X},
journal = {Applied Acoustics},
keywords = {Aircraft noise,Flight altitude,Low-density area,Noise attenuation,Urban morphology},
month = {oct},
pages = {73--82},
title = {{Influence of mesoscale urban morphology on the spatial noise attenuation of flyover aircrafts}},
url = {http://www.sciencedirect.com/science/article/pii/S0003682X1300282X},
volume = {84},
year = {2014}
}
@article{Torija2014,
abstract = {To ensure appropriate soundscape management in urban environments, the urban-planning authorities need a range of tools that enable such a task to be performed. An essential step during the management of urban areas from a sound standpoint should be the evaluation of the soundscape in such an area. In this sense, it has been widely acknowledged that a subjective and acoustical categorization of a soundscape is the first step to evaluate it, providing a basis for designing or adapting it to match people's expectations as well. In this sense, this work proposes a model for automatic classification of urban soundscapes. This model is intended for the automatic classification of urban soundscapes based on underlying acoustical and perceptual criteria. Thus, this classification model is proposed to be used as a tool for a comprehensive urban soundscape evaluation. Because of the great complexity associated with the problem, two machine learning techniques, Support Vector Machines (SVM) and Support Vector Machines trained with Sequential Minimal Optimization (SMO), are implemented in developing model classification. The results indicate that the SMO model outperforms the SVM model in the specific task of soundscape classification. With the implementation of the SMO algorithm, the classification model achieves an outstanding performance (91.3{\%} of instances correctly classified).},
author = {Torija, Antonio J and Ruiz, Diego P and Ramos-Ridao, Angel F},
issn = {1879-1026},
journal = {The Science of the total environment},
keywords = {Acoustical assessment,Classification model,Sequential Minimal Optimization,Soundscape classifier,Soundscape evaluation,Support Vector Machines},
month = {jun},
pages = {440--51},
title = {{A tool for urban soundscape evaluation applying Support Vector Machines for developing a soundscape classification model.}},
url = {http://www.sciencedirect.com/science/article/pii/S0048969713008929},
volume = {482-483},
year = {2014}
}
