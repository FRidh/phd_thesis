volume = {329},
year = {2010}
}
@article{Lercher2013,
abstract = {Although the majority of studies on community noise levels and children's physiological stress responses are positive, effect sizes vary considerably, and some studies do not confirm these effects. Employing a contextual perspective congruent with soundscapes, a carefully constructed sample of children (N = 115, M = 10.1 yr) living in households in relatively high ({\textgreater}60 dBA) or low ({\textless}50 dBA) noise areas created by proximity to major traffic arterials in Austria was reanalyzed. Several personal and environmental factors known to affect resting cardiovascular parameters measured under well-controlled, clinical conditions were incorporated into the analyses. Children with premature births and elevated chronic stress (i.e., overnight cortisol) were more susceptible to adverse blood pressure responses to road traffic noise. Residence in a multi-dwelling unit as well as standardized assessments of perceived quietness of the area did not modify the traffic noise impacts but each had its own, independent effect on resting blood pressure. A primary air pollutant associated with traffic volume (NO2) had no influence on any of these results. The scope of environmental noise assessment and management would benefit from incorporation of a more contextualized approach as suggested by the soundscape perspective.},
author = {Lercher, Peter and Evans, Gary W and Widmann, Ulrich},
file = {::},
issn = {1520-8524},
journal = {The Journal of the Acoustical Society of America},
keywords = {Austria,Blood Pressure,Blood Pressure: physiology,Child,Educational Status,Environmental Health,Female,Humans,Hydrocortisone,Hydrocortisone: blood,Male,Mothers,Noise, Transportation,Noise, Transportation: adverse effects,Questionnaires,Risk Factors,Sound Spectrography,Urban Population},
month = {jul},
number = {1},
pages = {773--81},
title = {{The ecological context of soundscapes for children's blood pressure.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4109089{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {134},
year = {2013}
}
@article{VanRenterghem2013a,
abstract = {Reduction of noise is one of the multiple benefits of building envelope greening measures. The potential of wall vegetation systems, green roofs, vegetated low screens at roof edges, and also combinations of such treatments, have been studied by means of combining 2D and 3D full-wave numerical methodologies. This study is concerned with road traffic noise propagation towards the traffic-free sides of inner-city buildings (courtyards). Preserving quietness at such locations has been shown before to be beneficial for the health and well-being of citizens. The results in this study show that green roofs have the highest potential to enhance quietness in courtyards. Favourable combinations of roof shape and green roofs have been identified. Vegetated fa{\c{c}}ades are most efficient when applied to narrow city canyons with otherwise acoustically hard fa{\c{c}}ade materials. Greening of the upper storey's in the street and (full) fa{\c{c}}ades in the courtyard itself is most efficient to achieve noise reduction. Low-height roof screens were shown to be effective when multiple screens are placed, but only on conditions that their faces are absorbing. The combination of different greening measures results in a lower combined effect than when the separate effects would have been linearly added. The combination of green roofs or wall vegetation with roof screens seems most interesting.},
author = {{Van Renterghem}, Timothy and Hornikx, Maarten and Forssen, Jens and Botteldooren, Dick},
issn = {03601323},
journal = {Building and Environment},
keywords = {Green roofs,Green walls,Noise reduction,Numerical simulations,Road traffic noise,Urban sound propagation},
month = {mar},
pages = {34--44},
title = {{The potential of building envelope greening to achieve quietness}},
url = {http://www.sciencedirect.com/science/article/pii/S036013231200323X},
volume = {61},
year = {2013}
}
