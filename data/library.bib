@article{Wilson2009,
abstract = {Quasi-wavelets (QWs) are similar to customary wavelets in that they are based on translations and dilations of a parent function; however, their positions and orientations are random. QWs are convenient for representing random fields with a self-similar structure. In this paper, a general, multi-dimensional treatment of QW fields is presented that includes scale-dependence in the number density and amplitude of the QWs. Previous QW formulations are extended to include anisotropy and correlations among several properties of the random fields. These extensions would be difficult (if not impossible) to achieve systematically by Fourier methods. As an example application, it is shown how QW models can be constructed to produce constant turbulent flux layers. Heat flux in buoyantly driven turbulence is modeled as a collection of QWs with predominantly horizontal rotation coupled with dipole scalar perturbations. Predictions for spectra in the presence of fluxes are obtained.},
author = {Wilson, D. Keith and Ostashev, Vladimir E. and Goedecke, George H.},
doi = {10.1016/j.probengmech.2008.09.002},
file = {:home/freddy/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wilson, Ostashev, Goedecke - 2009 - Quasi-wavelet formulations of turbulence and other random fields with correlated properties.pdf:pdf},
issn = {02668920},
journal = {Probabilistic Engineering Mechanics},
keywords = {Kinematic simulation,Self-similar representations,Spectral modeling},
number = {3},
pages = {343--357},
publisher = {Elsevier Ltd},
title = {{Quasi-wavelet formulations of turbulence and other random fields with correlated properties}},
url = {http://dx.doi.org/10.1016/j.probengmech.2008.09.002},
volume = {24},
year = {2009}
}
@inproceedings{Minard2016,
abstract = {Over the past decades, aircraft flyover noise has become a source of annoyance of increasing importance for people living near airports. For this reason, numerous studies are dedicated to the understanding of the relation between the acoustic characteristics of the flyover sound and the auditory sensations they provoke. Here is presented a method of sound simulation allowing manufacturers and researchers to create and listen to realistic binaural sound signals on the basis of a simple description of sources, trajectories and environment. The source is defined as a collection of noise and tonal components. Propagation (atmospheric absorption, geometrical spreading, Doppler effect, ground reflection, turbulence) is modeled under simplifying hypotheses. Binaural sound synthesis is realized with an additive method and Head-Related Transfer Functions, which are adapted in real-time according to headtracking data. This method was successfully used in a collaborative scientific project dealing with the influence of spectro-temporal aspects of flyover sound on listeners' unpleasantness. I.},
