journal = {The Journal of the Acoustical Society of America},
number = {1},
pages = {32--41},
pmid = {17297758},
title = {{Review of field studies of aircraft noise-induced sleep disturbance}},
url = {http://asa.scitation.org/doi/10.1121/1.2400613},
volume = {121},
year = {2007}
}
